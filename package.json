{
  "name": "web-sdk",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/",
  "repository": "https://github.com/CrazyMan-IK/web-sdk",
  "author": "CrazyMan-IK <superdanik2004@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && webpack --config webpack.prod.js",
    "build:dev": "rimraf ./dist && webpack --config webpack.dev.js",
    "serve": "webpack serve --config webpack.dev.js",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.{ts,scss,html}'",
    "deploy": "git subtree split --prefix dist -b build && git push -f origin build:main && git branch -D build"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-strip-block": "^0.3.0"
  },
  "dependencies": {
    "strongly-typed-events": "^3.0.9"
  },
  "packageManager": "yarn@3.6.0"
}
